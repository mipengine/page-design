@import './variables.styl'

#header
    border-bottom 1px solid #f1f1f1
    height header-hegiht
    line-height header-hegiht
    padding-left margin-left
    padding-right margin-right
    .left, .right
        position absolute
        top 0
        z-index 10
    .left
        left margin-left
        text-align left
    .right
        right margin-right
    a
        width 22px
        height header-hegiht
        line-height header-hegiht
        color #333
    h1
        margin 0
        padding 0 50px
        text-align center
